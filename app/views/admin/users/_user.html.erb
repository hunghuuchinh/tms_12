<li>
  <%= gravatar_for user, size: 50 %>
  <%= link_to user.name,[:admin, user] %>
    <% if current_user.admin? && !current_user?(user) %>
       |<%= link_to "edit ",[:edit, :admin, user]%> |
      <%= link_to "update", '#' %> |
      <%= link_to "delete",[:admin,user], method: :delete,
                                  data: { confirm: "You sure?" } %>
    <% end %>

</li>